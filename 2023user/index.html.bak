    <script>
      document.addEventListener('DOMContentLoaded', function () {
        const tabItems = document.querySelectorAll('.tab > li');
        const tabSections = document.querySelectorAll('section.tab-section');
        onOpenSection();

        tabItems.forEach((item, index) => {
          item.addEventListener('click', onTabsClick);
        });

        function onTabsClick(e) {
          tabItems.forEach((item) => item.classList.remove('active'));
          e.target.classList.add('active');
          onOpenSection();
        }

        function onOpenSection() {
          tabItems.forEach((item, index) => {
            item.classList.contains('active') ? (tabSections[index].style.display = 'block') : (tabSections[index].style.display = 'none');
          });
        }
      });
    </script>
